[user]
  email = lotreal@gmail.com
  name = luo tao
[alias]
  co = checkout
  st = status
  br = branch
  w = whatchanged
	ci = commit
	diffis = diff --ignore-space-change
	diffia = diff --ignore-all-space

[apply]
  whitespace = nowarn

[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = yellow bold
	changed = green bold
	untracked = cyan bold

[merge]
  summary = true
[push]
  default = tracking
[remote "origin"]
	push = HEAD
[github]
	user = rmm5t
[diff]
  ignoresubmodules = untracked