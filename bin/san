#!/bin/bash
# if [ "$(basename $0)" != "install_all.sh" ]; then
#     source ${0%/*}/config.sh
# fi

xpath()
{
    for line in $(tar tf $1); do
        [ "${line//\//}" != $line ] && break
    done
    echo $line | sed "s/^\([^/]*\)\(.*\)$/\1/"
}

xpath archives/memcache-2.2.5.tgz
p=$(xpath archives/PDO_MYSQL-1.0.2.tgz)

echo -----------
echo $p
